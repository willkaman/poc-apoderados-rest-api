#!groovy
@Library("utils") _
springBootPipeline {
	// Atributo que permite identificar el proyecto del componente de software a liberar. Ej: Proyecto Centro de Negocio - Servicio REST mediante el cual se pueden crear eventos en la plataforma de eventos de Banchile( obligatorio)
        projectName = ""
        // Atributo que identifica el ID al sistema al cual pertenece el componente. Los valores posibles corresponden al campo ID en el siguiente link http://confluence.bantcent.cl/display/DTB/2.1.3.6+Sistemas
        idSistema = ""
	// Atributo que identifica el artifactId asociado al modulo a liberar, no del pom parent. Ej: cl.banchile.cliente.banchilepay.exp-ear (obligatorio)
        artifactId = ""
	// Atributo que identifica el tipo de extension del binario ej:ear, jar. (obligatorio)
        extension= "jar"

} 